{
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[xml]": {
        "editor.defaultFormatter": "rogalmic.vscode-xml-complete"
    },
    "chaliceIcons.showArrows": true,
    "csharp.semanticHighlighting.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "'Cascadia Code', 'Font Awesome 6 Brands', 'Font Awesome 6 Free', 'Font Awesome 6 Free Solid'",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.fontSize": 13,
    "editor.fontWeight": 350,
    "editor.lineHeight": 1.2,
    "editor.minimap.enabled": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.stickyScroll.enabled": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment, string",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword",
                "settings": {
                    "fontStyle": "bold italic"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.enableTrash": false,
    "git.openRepositoryInParentFolders": "never",
    "gitlens.telemetry.enabled": false,
    "go.gopath": "~/.go",
    "html.completion.attributeDefaultValue": "singlequotes",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "omnisharp.autoStart": false,
    "omnisharp.enableDecompilationSupport": true,
    "rust-analyzer.completion.autoimport.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.renderColons": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontFamily": "'Cascadia Code', 'Font Awesome 6 Brands', 'Font Awesome 6 Free', 'Font Awesome 6 Free Solid'",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeight": 350,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "todo-tree.highlights.useColourScheme": true,
    "typescript.preferences.quoteStyle": "single",
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "window.commandCenter": false,
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#37384d"
    },
    "workbench.editor.empty.hint": "hidden",
    "workbench.iconTheme": "chalice-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "always"
}
